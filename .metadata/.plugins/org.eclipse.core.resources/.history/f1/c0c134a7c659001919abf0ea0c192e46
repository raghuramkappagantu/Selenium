package Demo;

public class ReverseNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num=1234,revNum=0;
		int digit;
		while(num>0)
		{
			digit=num%10;
			revNum=revNum*10+digit;
			num=num/10;
			System.out.println("Reveresed Num is: " + revNum);
			
		}
		

	}

}
